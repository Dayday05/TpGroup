package fr.formation;

import java.util.ArrayList;
import java.util.List;

public class Students {
	
	public static void main(String[] args) {
		
		List <IStudent> studentsName = new ArrayList<IStudent>();
		
		studentsName.add("Aurélien");
		studentsName.add("Pierre");
		studentsName.add("Bayrem");
		studentsName.add("Jennifer");
		studentsName.add("Héléne");
		studentsName.add("David");
		studentsName.add("Julien");
		
		List <IStudent> studentsGroup = new ArrayList <IStudent>();
		
		studentsGroup.add(3);
		studentsGroup.add(1);
		studentsGroup.add(1);
		studentsGroup.add(2);
		studentsGroup.add(3);
		studentsGroup.add(2);
		studentsGroup.add(2);
		
		ArrayList <IStudent> studentsIsInternal = new ArrayList <IStudent>();
		
		studentsIsInternal.add(false);
		studentsIsInternal.add(true);
		studentsIsInternal.add(true);
		studentsIsInternal.add(false);
		studentsIsInternal.add(true);
		studentsIsInternal.add(false);
		studentsIsInternal.add(true);
		
		List <iStudent> listStudents = new ArrayList <String>();
		
		listStudents.addAll(studentsName);
		listStudents.addAll(studentsGroup);
		listStudents.addAll(studentsIsInternal);
		
		System.out.println(listStudents);
		
		for(String temp : listStudents){
			System.out.println(temp);
		}
		
	}
}
